<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.10">
  <defaultSuffix><%= node[:kitchen_jenkins][:mail][:suffix] %></defaultSuffix>
  <replyToAddress><%= node[:kitchen_jenkins][:mail][:reply] %></replyToAddress>
  <smtpHost><%= node[:kitchen_jenkins][:mail][:host] %></smtpHost>
  <useSsl>false</useSsl>
  <smtpPort><%= node[:kitchen_jenkins][:mail][:port] %></smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>